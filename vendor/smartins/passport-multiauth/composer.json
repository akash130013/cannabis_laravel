{
    "name": "smartins/passport-multiauth",
    "description": "Add support to multi-auth on Laravel Passport",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "passport", "multi-auth"],
    "authors": [
        {
            "name": "Samuel Martins",
            "email": "sfelix_martins@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "laravel/passport": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.7",
        "mockery/mockery": "^1.0",
        "orchestra/database": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "SMartins\\PassportMultiauth\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "SMartins\\PassportMultiauth\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "SMartins\\PassportMultiauth\\Providers\\MultiauthServiceProvider"
            ]
        }
    }

}
